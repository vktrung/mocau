# Database Configuration (VPS MySQL đã cài sẵn)
DB_CONN=root:password@tcp(localhost:3306)/base_project?charset=utf8mb4&parseTime=True&loc=Local

# JWT Secret Key (thay đổi thành key mạnh trong production)
SECRET=your_super_secret_jwt_key_here

# Swagger Host (cho documentation)
SWAGGER_HOST=your_vps_ip:3000

# MySQL Configuration (for GitHub Actions)
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=base_project
MYSQL_USER=app_user
MYSQL_PASSWORD=app_password

# Server Configuration (optional)
PORT=3000
GIN_MODE=debug
